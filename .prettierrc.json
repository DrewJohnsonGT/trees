{
  "trailingComma": "all",
  "tabWidth": 2,
  "singleQuote": true,
  "jsxBracketSameLine": true,
  "proseWrap": "always",
  "endOfLine": "lf",
  "importOrder": [
    "@mantine/core/styles.css",
    "^react",
    "<THIRD_PARTY_MODULES>",
    "^[~/]",
    "^[./]"
  ],
  "importOrderSeparation": false,
  "importOrderSortSpecifiers": false,
  "plugins": ["@trivago/prettier-plugin-sort-imports"]
}
